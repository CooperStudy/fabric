	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[29], 1);
		return fmt.Errorf("Invalid Proposal's SignatureHeader during channelless check policy with policy [%s]: [%s]", policyName, err)
	}

