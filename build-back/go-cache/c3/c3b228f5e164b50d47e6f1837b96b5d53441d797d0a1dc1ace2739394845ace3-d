	Count     [2]uint32
	Pos       [3 * 2]uint32
	NumStmt   [2]uint16
} {
	Pos: [3 * 2]uint32{
		42, 44, 0x2003b, // [0]
		47, 62, 0x2003e, // [1]
	},
	NumStmt: [2]uint16{
		1, // 0
		11, // 1
	},
}
var _ = _cover_atomic_.LoadUint32
