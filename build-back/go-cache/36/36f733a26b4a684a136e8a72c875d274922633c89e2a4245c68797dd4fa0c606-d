var putilsLogger = flogging.MustGetLogger("protoutils")

// validateChaincodeProposalMessage checks the validity of a Proposal message of type CHAINCODE
func validateChaincodeProposalMessage(prop *pb.Proposal, hdr *common.Header) (*pb.ChaincodeHeaderExtension, error) {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[0], 1);
	if prop == nil || hdr == nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[5], 1);
		return nil, errors.New("nil arguments")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[1], 1);putilsLogger.Debugf("validateChaincodeProposalMessage starts for proposal %p, header %p", prop, hdr)

	// 4) based on the header type (assuming it's CHAINCODE), look at the extensions
	chaincodeHdrExt, err := utils.GetChaincodeHeaderExtension(hdr)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[6], 1);
		return nil, errors.New("invalid header extension for type CHAINCODE")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[2], 1);if chaincodeHdrExt.ChaincodeId == nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[7], 1);
		return nil, errors.New("ChaincodeHeaderExtension.ChaincodeId is nil")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[3], 1);putilsLogger.Debugf("validateChaincodeProposalMessage info: header extension references chaincode %s", chaincodeHdrExt.ChaincodeId)

	//    - ensure that the chaincodeID is correct (?)
	// TODO: should we even do this? If so, using which interface?

	//    - ensure that the visibility field has some value we understand
	// currently the fabric only supports full visibility: this means that
	// there are no restrictions on which parts of the proposal payload will
	// be visible in the final transaction; this default approach requires
	// no additional instructions in the PayloadVisibility field which is
	// therefore expected to be nil; however the fabric may be extended to
	// encode more elaborate visibility mechanisms that shall be encoded in
	// this field (and handled appropriately by the peer)
	if chaincodeHdrExt.PayloadVisibility != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[8], 1);
		return nil, errors.New("invalid payload visibility field")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[4], 1);return chaincodeHdrExt, nil
}

// ValidateProposalMessage checks the validity of a SignedProposal message
// this function returns Header and ChaincodeHeaderExtension messages since they
// have been unmarshalled and validated
func ValidateProposalMessage(signedProp *pb.SignedProposal) (*pb.Proposal, *common.Header, *pb.ChaincodeHeaderExtension, error) {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[9], 1);
	if signedProp == nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[16], 1);
		return nil, nil, nil, errors.New("nil arguments")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[10], 1);putilsLogger.Debugf("ValidateProposalMessage starts for signed proposal %p", signedProp)

	// extract the Proposal message from signedProp
	prop, err := utils.GetProposal(signedProp.ProposalBytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[17], 1);
		return nil, nil, nil, err
	}

	// 1) look at the ProposalHeader
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[11], 1);hdr, err := utils.GetHeader(prop.Header)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[18], 1);
		return nil, nil, nil, err
	}

	// validate the header
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[12], 1);chdr, shdr, err := validateCommonHeader(hdr)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[19], 1);
		return nil, nil, nil, err
	}

	// validate the signature
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[13], 1);err = checkSignatureFromCreator(shdr.Creator, signedProp.Signature, signedProp.ProposalBytes, chdr.ChannelId)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[20], 1);
		// log the exact message on the peer but return a generic error message to
		// avoid malicious users scanning for channels
		putilsLogger.Warningf("channel [%s]: %s", chdr.ChannelId, err)
		sId := &msp.SerializedIdentity{}
		err := proto.Unmarshal(shdr.Creator, sId)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[22], 1);
			// log the error here as well but still only return the generic error
			err = errors.Wrap(err, "could not deserialize a SerializedIdentity")
			putilsLogger.Warningf("channel [%s]: %s", chdr.ChannelId, err)
		}
		_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[21], 1);return nil, nil, nil, errors.Errorf("access denied: channel [%s] creator org [%s]", chdr.ChannelId, sId.Mspid)
	}

	// Verify that the transaction ID has been computed properly.
	// This check is needed to ensure that the lookup into the ledger
	// for the same TxID catches duplicates.
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[14], 1);err = utils.CheckTxID(
		chdr.TxId,
		shdr.Nonce,
		shdr.Creator)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[23], 1);
		return nil, nil, nil, err
	}

	// continue the validation in a way that depends on the type specified in the header
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[15], 1);switch common.HeaderType(chdr.Type) {
	case common.HeaderType_CONFIG:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[24], 1);
		//which the types are different the validation is the same
		//viz, validate a proposal to a chaincode. If we need other
		//special validation for confguration, we would have to implement
		//special validation
		fallthrough
	case common.HeaderType_ENDORSER_TRANSACTION:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[25], 1);
		// validation of the proposal message knowing it's of type CHAINCODE
		chaincodeHdrExt, err := validateChaincodeProposalMessage(prop, hdr)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[28], 1);
			return nil, nil, nil, err
		}

		_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[26], 1);return prop, hdr, chaincodeHdrExt, err
	default:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[27], 1);
		//NOTE : we proably need a case
		return nil, nil, nil, errors.Errorf("unsupported proposal type %d", common.HeaderType(chdr.Type))
	}
}

// given a creator, a message and a signature,
// this function returns nil if the creator
// is a valid cert and the signature is valid
func checkSignatureFromCreator(creatorBytes []byte, sig []byte, msg []byte, ChainID string) error {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[29], 1);
	putilsLogger.Debugf("begin")

	// check for nil argument
	if creatorBytes == nil || sig == nil || msg == nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[35], 1);
		return errors.New("nil arguments")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[30], 1);mspObj := mspmgmt.GetIdentityDeserializer(ChainID)
	if mspObj == nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[36], 1);
		return errors.Errorf("could not get msp for channel [%s]", ChainID)
	}

	// get the identity of the creator
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[31], 1);creator, err := mspObj.DeserializeIdentity(creatorBytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[37], 1);
		return errors.WithMessage(err, "MSP error")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[32], 1);putilsLogger.Debugf("creator is %s", creator.GetIdentifier())

	// ensure that creator is a valid certificate
	err = creator.Validate()
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[38], 1);
		return errors.WithMessage(err, "creator certificate is not valid")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[33], 1);putilsLogger.Debugf("creator is valid")

	// validate the signature
	err = creator.Verify(msg, sig)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[39], 1);
		return errors.WithMessage(err, "creator's signature over the proposal is not valid")
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[34], 1);putilsLogger.Debugf("exits successfully")

	return nil
}

