			return nil, err
		}

		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[119], 1);return pem.EncodeToMemory(block), nil

	default:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[120], 1);
		return nil, errors.New("Invalid key type. It must be *ecdsa.PublicKey")
	}
}

