	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[27], 1);
		return err
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[24], 1);if !ok {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[28], 1);
		return errors.Errorf("Attribute '%s' was not found", attrName)
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[25], 1);if val != attrValue {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[29], 1);
		return errors.Errorf("Attribute '%s' equals '%s', not '%s'", attrName, val, attrValue)
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[26], 1);return nil
