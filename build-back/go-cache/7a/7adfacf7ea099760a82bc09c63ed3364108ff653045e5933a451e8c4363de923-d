	}

	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[2], 1);if policyName == "" {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[10], 1);
		return fmt.Errorf("Invalid policy name during check policy on channel [%s]. Name must be different from nil.", channelID)
	}

	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[3], 1);if signedProp == nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[11], 1);
		return fmt.Errorf("Invalid signed proposal during check policy on channel [%s] with policy [%s]", channelID, policyName)
	}

	// Get Policy
	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[4], 1);policyManager, _ := p.channelPolicyManagerGetter.Manager(channelID)
	if policyManager == nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[12], 1);
		return fmt.Errorf("Failed to get policy manager for channel [%s]", channelID)
	}

	// Prepare SignedData
	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[5], 1);proposal, err := utils.GetProposal(signedProp.ProposalBytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[13], 1);
		return fmt.Errorf("Failing extracting proposal during check policy on channel [%s] with policy [%s]: [%s]", channelID, policyName, err)
	}

	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[6], 1);header, err := utils.GetHeader(proposal.Header)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[14], 1);
		return fmt.Errorf("Failing extracting header during check policy on channel [%s] with policy [%s]: [%s]", channelID, policyName, err)
	}

	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[7], 1);shdr, err := utils.GetSignatureHeader(header.SignatureHeader)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[15], 1);
		return fmt.Errorf("Invalid Proposal's SignatureHeader during check policy on channel [%s] with policy [%s]: [%s]", channelID, policyName, err)
	}

	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[8], 1);sd := []*common.SignedData{{
		Data:      signedProp.ProposalBytes,
		Identity:  shdr.Creator,
		Signature: signedProp.Signature,
	}}

	return p.CheckPolicyBySignedData(channelID, policyName, sd)
}

// CheckPolicyNoChannel checks that the passed signed proposal is valid with the respect to
// passed policy on the local MSP.
func (p *policyChecker) CheckPolicyNoChannel(policyName string, signedProp *pb.SignedProposal) error {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[16], 1);
	if policyName == "" {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[25], 1);
