		return "", false, nil
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[21], 1);return c.attrs.Value(attrName)
}

// AssertAttributeValue checks to see if an attribute value equals the specified value
func (c *clientIdentityImpl) AssertAttributeValue(attrName, attrValue string) error {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[23], 1);
