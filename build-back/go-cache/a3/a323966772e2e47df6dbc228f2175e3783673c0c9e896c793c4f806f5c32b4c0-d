func (chn *channelImpl) topic() string {_cover_atomic_.AddUint32(&GoCover_1_623633326437306563303132.Count[1], 1);
	return chn.tpc
}

// partition returns the Kafka partition where this channel resides.
func (chn *channelImpl) partition() int32 {_cover_atomic_.AddUint32(&GoCover_1_623633326437306563303132.Count[2], 1);
	return chn.prt
}

// String returns a string identifying the Kafka topic/partition corresponding
// to this channel.
func (chn *channelImpl) String() string {_cover_atomic_.AddUint32(&GoCover_1_623633326437306563303132.Count[3], 1);
