/*
 * The attrmgr package contains utilities for managing attributes.
 * Attributes are added to an X509 certificate as an extension.
 */

package attrmgr; import _cover_atomic_ "sync/atomic"

import (
	"crypto/x509"
	"crypto/x509/pkix"
	"encoding/asn1"
	"encoding/json"
	"fmt"

	"github.com/golang/protobuf/proto"
	"github.com/hyperledger/fabric/protos/msp"
	"github.com/pkg/errors"
)

var (
	// AttrOID is the ASN.1 object identifier for an attribute extension in an
	// X509 certificate
	AttrOID = asn1.ObjectIdentifier{1, 2, 3, 4, 5, 6, 7, 8, 1}
	// AttrOIDString is the string version of AttrOID
	AttrOIDString = "1.2.3.4.5.6.7.8.1"
)

// Attribute is a name/value pair
type Attribute interface {
	// GetName returns the name of the attribute
	GetName() string
	// GetValue returns the value of the attribute
	GetValue() string
}

// AttributeRequest is a request for an attribute
type AttributeRequest interface {
	// GetName returns the name of an attribute
	GetName() string
	// IsRequired returns true if the attribute is required
	IsRequired() bool
}

// New constructs an attribute manager
func New() *Mgr {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[0], 1); return &Mgr{} }

// Mgr is the attribute manager and is the main object for this package
type Mgr struct{}

// ProcessAttributeRequestsForCert add attributes to an X509 certificate, given
// attribute requests and attributes.
func (mgr *Mgr) ProcessAttributeRequestsForCert(requests []AttributeRequest, attributes []Attribute, cert *x509.Certificate) error {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[1], 1);
	attrs, err := mgr.ProcessAttributeRequests(requests, attributes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[3], 1);
		return err
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[2], 1);return mgr.AddAttributesToCert(attrs, cert)
}

// ProcessAttributeRequests takes an array of attribute requests and an identity's attributes
// and returns an Attributes object containing the requested attributes.
func (mgr *Mgr) ProcessAttributeRequests(requests []AttributeRequest, attributes []Attribute) (*Attributes, error) {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[4], 1);
	attrsMap := map[string]string{}
	attrs := &Attributes{Attrs: attrsMap}
	missingRequiredAttrs := []string{}
	// For each of the attribute requests
	for _, req := range requests {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[7], 1);
		// Get the attribute
		name := req.GetName()
		attr := getAttrByName(name, attributes)
		if attr == nil {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[9], 1);
			if req.IsRequired() {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[11], 1);
				// Didn't find attribute and it was required; return error below
				missingRequiredAttrs = append(missingRequiredAttrs, name)
			}
			// Skip attribute requests which aren't required
			_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[10], 1);continue
		}
		_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[8], 1);attrsMap[name] = attr.GetValue()
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[5], 1);if len(missingRequiredAttrs) > 0 {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[12], 1);
		return nil, errors.Errorf("The following required attributes are missing: %+v",
			missingRequiredAttrs)
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[6], 1);return attrs, nil
}

// AddAttributesToCert adds public attribute info to an X509 certificate.
func (mgr *Mgr) AddAttributesToCert(attrs *Attributes, cert *x509.Certificate) error {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[13], 1);
	buf, err := json.Marshal(attrs)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[15], 1);
		return errors.Wrap(err, "Failed to marshal attributes")
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[14], 1);ext := pkix.Extension{
		Id:       AttrOID,
		Critical: false,
		Value:    buf,
	}
	cert.Extensions = append(cert.Extensions, ext)
	return nil
}

// GetAttributesFromCert gets the attributes from a certificate.
func (mgr *Mgr) GetAttributesFromCert(cert *x509.Certificate) (*Attributes, error) {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[16], 1);
	// Get certificate attributes from the certificate if it exists
	buf, err := getAttributesFromCert(cert)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[19], 1);
		return nil, err
	}
	// Unmarshal into attributes object
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[17], 1);attrs := &Attributes{}
	if buf != nil {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[20], 1);
		err := json.Unmarshal(buf, attrs)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[21], 1);
			return nil, errors.Wrap(err, "Failed to unmarshal attributes from certificate")
		}
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[18], 1);return attrs, nil
}

