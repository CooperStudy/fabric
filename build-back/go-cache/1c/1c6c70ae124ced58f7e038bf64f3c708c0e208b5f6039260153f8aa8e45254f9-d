// Initialize the client
func (c *clientIdentityImpl) init() error {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[31], 1);
	signingID, err := c.getIdentity()
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[36], 1);
		return err
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[32], 1);c.mspID = signingID.GetMspid()
	idbytes := signingID.GetIdBytes()
	block, _ := pem.Decode(idbytes)
	if block == nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[37], 1);
		err := c.getAttributesFromIdemix()
		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[39], 1);
			return errors.WithMessage(err, "identity bytes are neither X509 PEM format nor an idemix credential")
		}
		_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[38], 1);return nil
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[33], 1);cert, err := x509.ParseCertificate(block.Bytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[40], 1);
		return errors.WithMessage(err, "failed to parse certificate")
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[34], 1);c.cert = cert
	attrs, err := attrmgr.New().GetAttributesFromCert(cert)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[41], 1);
		return errors.WithMessage(err, "failed to get attributes from the transaction invoker's certificate")
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[35], 1);c.attrs = attrs
	return nil
}

// Unmarshals the bytes returned by ChaincodeStubInterface.GetCreator method and
// returns the resulting msp.SerializedIdentity object
func (c *clientIdentityImpl) getIdentity() (*msp.SerializedIdentity, error) {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[42], 1);
	sid := &msp.SerializedIdentity{}
	creator, err := c.stub.GetCreator()
	if err != nil || creator == nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[45], 1);
		return nil, errors.WithMessage(err, "failed to get transaction invoker's identity from the chaincode stub")
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[43], 1);err = proto.Unmarshal(creator, sid)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[46], 1);
		return nil, errors.Wrap(err, "failed to unmarshal transaction invoker's identity")
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[44], 1);return sid, nil
}

func (c *clientIdentityImpl) getAttributesFromIdemix() error {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[47], 1);
