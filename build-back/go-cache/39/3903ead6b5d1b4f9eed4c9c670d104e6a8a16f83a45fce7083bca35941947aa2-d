	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[10], 1);
		return nil, ConfigTxFileNotFound(err.Error())
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[9], 1);return utils.UnmarshalEnvelope(cftx)
}

func sanityCheckAndSignConfigTx(envConfigUpdate *cb.Envelope) (*cb.Envelope, error) {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[11], 1);
	payload, err := utils.ExtractPayload(envConfigUpdate)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[21], 1);
		return nil, InvalidCreateTx("bad payload")
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[12], 1);if payload.Header == nil || payload.Header.ChannelHeader == nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[22], 1);
		return nil, InvalidCreateTx("bad header")
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[13], 1);ch, err := utils.UnmarshalChannelHeader(payload.Header.ChannelHeader)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[23], 1);
		return nil, InvalidCreateTx("could not unmarshall channel header")
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[14], 1);if ch.Type != int32(cb.HeaderType_CONFIG_UPDATE) {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[24], 1);
		return nil, InvalidCreateTx("bad type")
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[15], 1);if ch.ChannelId == "" {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[25], 1);
		return nil, InvalidCreateTx("empty channel id")
	}

	// Specifying the chainID on the CLI is usually redundant, as a hack, set it
	// here if it has not been set explicitly
	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[16], 1);if channelID == "" {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[26], 1);
		channelID = ch.ChannelId
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[17], 1);if ch.ChannelId != channelID {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[27], 1);
		return nil, InvalidCreateTx(fmt.Sprintf("mismatched channel ID %s != %s", ch.ChannelId, channelID))
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[18], 1);configUpdateEnv, err := configtx.UnmarshalConfigUpdateEnvelope(payload.Data)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[28], 1);
		return nil, InvalidCreateTx("Bad config update env")
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[19], 1);signer := localsigner.NewSigner()
	sigHeader, err := signer.NewSignatureHeader()
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[29], 1);
		return nil, err
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[20], 1);configSig := &cb.ConfigSignature{
		SignatureHeader: utils.MarshalOrPanic(sigHeader),
	}

	configSig.Signature, err = signer.Sign(util.ConcatenateBytes(configSig.SignatureHeader, configUpdateEnv.ConfigUpdate))

	configUpdateEnv.Signatures = append(configUpdateEnv.Signatures, configSig)

	return utils.CreateSignedEnvelope(cb.HeaderType_CONFIG_UPDATE, channelID, signer, configUpdateEnv, 0, 0)
}

func sendCreateChainTransaction(cf *ChannelCmdFactory) error {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[30], 1);
	var err error
	var chCrtEnv *cb.Envelope

	if channelTxFile != "" {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[34], 1);
		if chCrtEnv, err = createChannelFromConfigTx(channelTxFile); err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[35], 1);
			return err
		}
	} else{ _cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[36], 1);{
		if chCrtEnv, err = createChannelFromDefaults(cf); err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[37], 1);
			return err
		}
	}}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[31], 1);if chCrtEnv, err = sanityCheckAndSignConfigTx(chCrtEnv); err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[38], 1);
		return err
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[32], 1);var broadcastClient common.BroadcastClient
	broadcastClient, err = cf.BroadcastFactory()
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[39], 1);
		return errors.WithMessage(err, "error getting broadcast client")
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[33], 1);defer broadcastClient.Close()
	err = broadcastClient.Send(chCrtEnv)

	return err
}

func executeCreate(cf *ChannelCmdFactory) error {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[40], 1);
	err := sendCreateChainTransaction(cf)
