func (uniquePvtData uniquePvtDataMap) updateUsingCollPvtData(collPvtData *rwset.CollectionPvtReadWriteSet,
	ns string, ver *version.Height) error {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[40], 1);

