func (uniquePvtData uniquePvtDataMap) updateUsingPvtWrite(pvtWrite *kvrwset.KVWrite,
	hashedCompositeKey privacyenabledstate.HashedCompositeKey, ver *version.Height) {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[45], 1);

	pvtData, ok := uniquePvtData[hashedCompositeKey]
	if !ok || pvtData.Version.Compare(ver) < 0 {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[46], 1);
		uniquePvtData[hashedCompositeKey] =
			&privacyenabledstate.PvtKVWrite{
				Key:      pvtWrite.Key,
				IsDelete: pvtWrite.IsDelete,
				Value:    pvtWrite.Value,
				Version:  ver,
			}
	}
}

func (uniquePvtData uniquePvtDataMap) findAndRemoveStalePvtData(db privacyenabledstate.DB) error {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[47], 1);
	// (1) load all committed versions
	if err := uniquePvtData.loadCommittedVersionIntoCache(db); err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[50], 1);
		return err
	}

	// (2) find and remove the stale data
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[48], 1);for hashedCompositeKey, pvtWrite := range uniquePvtData {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[51], 1);
		isStale, err := checkIfPvtWriteIsStale(&hashedCompositeKey, pvtWrite, db)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[53], 1);
			return err
		}
		_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[52], 1);if isStale {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[54], 1);
			delete(uniquePvtData, hashedCompositeKey)
		}
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[49], 1);return nil
}

func (uniquePvtData uniquePvtDataMap) loadCommittedVersionIntoCache(db privacyenabledstate.DB) error {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[55], 1);
	// Note that ClearCachedVersions would not be called till we validate and commit these
	// pvt data of old blocks. This is because only during the exclusive lock duration, we
	// clear the cache and we have already acquired one before reaching here.
	var hashedCompositeKeys []*privacyenabledstate.HashedCompositeKey
	for hashedCompositeKey := range uniquePvtData {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[58], 1);
		// tempKey ensures a different pointer is added to the slice for each key
		tempKey := hashedCompositeKey
		hashedCompositeKeys = append(hashedCompositeKeys, &tempKey)
	}

	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[56], 1);err := db.LoadCommittedVersionsOfPubAndHashedKeys(nil, hashedCompositeKeys)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[59], 1);
		return err
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[57], 1);return nil
}

func checkIfPvtWriteIsStale(hashedKey *privacyenabledstate.HashedCompositeKey,
	kvWrite *privacyenabledstate.PvtKVWrite, db privacyenabledstate.DB) (bool, error) {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[60], 1);

	ns := hashedKey.Namespace
	coll := hashedKey.CollectionName
	keyHashBytes := []byte(hashedKey.KeyHash)
	committedVersion, err := db.GetKeyHashVersion(ns, coll, keyHashBytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[66], 1);
		return true, err
	}

	// for a deleted hashedKey, we would get a nil committed version. Note that
	// the hashedKey was deleted because either it got expired or was deleted by
	// the chaincode itself.
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[61], 1);if committedVersion == nil && kvWrite.IsDelete {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[67], 1);
		return false, nil
	}

	/*
		TODO: FAB-12922
		In the first round, we need to the check version of passed pvtData
		against the version of pvtdata stored in the stateDB. In second round,
		for the remaining pvtData, we need to check for stalenss using hashed
		version. In the third round, for the still remaining pvtdata, we need
		to check against hashed values. In each phase we would require to
		perform bulkload of relevant data from the stateDB.
		committedPvtData, err := db.GetPrivateData(ns, coll, kvWrite.Key)
		if err != nil {
			return false, err
		}
		if committedPvtData.Version.Compare(kvWrite.Version) > 0 {
			return false, nil
		}
	*/
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[62], 1);if version.AreSame(committedVersion, kvWrite.Version) {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[68], 1);
		return false, nil
	}

	// due to metadata updates, we could get a version
	// mismatch between pvt kv write and the committed
	// hashedKey. In this case, we must compare the hash
	// of the value. If the hash matches, we should update
	// the version number in the pvt kv write and return
	// true as the validation result
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[63], 1);vv, err := db.GetValueHash(ns, coll, keyHashBytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[69], 1);
		return true, err
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[64], 1);if bytes.Equal(vv.Value, util.ComputeHash(kvWrite.Value)) {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[70], 1);
		// if hash of value matches, update version
		// and return true
		kvWrite.Version = vv.Version // side effect
		// (checkIfPvtWriteIsStale should not be updating the state)
		return false, nil
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[65], 1);return true, nil
}

func (uniquePvtData uniquePvtDataMap) transformToUpdateBatch() *privacyenabledstate.UpdateBatch {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[71], 1);
	batch := privacyenabledstate.NewUpdateBatch()
	for hashedCompositeKey, pvtWrite := range uniquePvtData {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[73], 1);
		ns := hashedCompositeKey.Namespace
		coll := hashedCompositeKey.CollectionName
		if pvtWrite.IsDelete {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[74], 1);
			batch.PvtUpdates.Delete(ns, coll, pvtWrite.Key, pvtWrite.Version)
		} else{ _cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[75], 1);{
			batch.PvtUpdates.Put(ns, coll, pvtWrite.Key, pvtWrite.Value, pvtWrite.Version)
		}}
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[72], 1);return batch
}

func (txmgr *LockBasedTxMgr) invokeNamespaceListeners() error {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[76], 1);
	for _, listener := range txmgr.stateListeners {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[78], 1);
		stateUpdatesForListener := extractStateUpdates(txmgr.current.batch, listener.InterestedInNamespaces())
		if len(stateUpdatesForListener) == 0 {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[81], 1);
			continue
		}
		_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[79], 1);txmgr.current.listeners = append(txmgr.current.listeners, listener)

		committedStateQueryExecuter := &queryutil.QECombiner{
			QueryExecuters: []queryutil.QueryExecuter{txmgr.db}}

		postCommitQueryExecuter := &queryutil.QECombiner{
			QueryExecuters: []queryutil.QueryExecuter{
				&queryutil.UpdateBatchBackedQueryExecuter{UpdateBatch: txmgr.current.batch.PubUpdates.UpdateBatch},
				txmgr.db,
			},
		}

		trigger := &ledger.StateUpdateTrigger{
			LedgerID:                    txmgr.ledgerid,
			StateUpdates:                stateUpdatesForListener,
			CommittingBlockNum:          txmgr.current.blockNum(),
			CommittedStateQueryExecutor: committedStateQueryExecuter,
			PostCommitQueryExecutor:     postCommitQueryExecuter,
		}
		if err := listener.HandleStateUpdates(trigger); err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[82], 1);
			return err
		}
		_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[80], 1);logger.Debugf("Invoking listener for state changes:%s", listener)
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[77], 1);return nil
}

// Shutdown implements method in interface `txmgmt.TxMgr`
func (txmgr *LockBasedTxMgr) Shutdown() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[83], 1);
	// wait for background go routine to finish else the timing issue causes a nil pointer inside goleveldb code
	// see FAB-11974
	txmgr.pvtdataPurgeMgr.WaitForPrepareToFinish()
	txmgr.db.Close()
}

// Commit implements method in interface `txmgmt.TxMgr`
func (txmgr *LockBasedTxMgr) Commit() error {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[84], 1);
	// we need to acquire a lock on oldBlockCommit. The following are the two reasons:
	// (1) the DeleteExpiredAndUpdateBookkeeping() would perform incorrect operation if
	//        toPurgeList is updated by RemoveStaleAndCommitPvtDataOfOldBlocks().
	// (2) RemoveStaleAndCommitPvtDataOfOldBlocks computes the update
	//     batch based on the current state and if we allow regular block commits at the
	//     same time, the former may overwrite the newer versions of the data and we may
	//     end up with an incorrect update batch.
	txmgr.oldBlockCommit.Lock()
	defer txmgr.oldBlockCommit.Unlock()
	logger.Debug("lock acquired on oldBlockCommit for committing regular updates to state database")

	// When using the purge manager for the first block commit after peer start, the asynchronous function
	// 'PrepareForExpiringKeys' is invoked in-line. However, for the subsequent blocks commits, this function is invoked
	// in advance for the next block
	if !txmgr.pvtdataPurgeMgr.usedOnce {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[91], 1);
		txmgr.pvtdataPurgeMgr.PrepareForExpiringKeys(txmgr.current.blockNum())
		txmgr.pvtdataPurgeMgr.usedOnce = true
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[85], 1);defer func() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[92], 1);
		txmgr.pvtdataPurgeMgr.PrepareForExpiringKeys(txmgr.current.blockNum() + 1)
		logger.Debugf("launched the background routine for preparing keys to purge with the next block")
		txmgr.reset()
	}()

	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[86], 1);logger.Debugf("Committing updates to state database")
	if txmgr.current == nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[93], 1);
		panic("validateAndPrepare() method should have been called before calling commit()")
	}

	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[87], 1);if err := txmgr.pvtdataPurgeMgr.DeleteExpiredAndUpdateBookkeeping(
		txmgr.current.batch.PvtUpdates, txmgr.current.batch.HashUpdates); err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[94], 1);
		return err
	}

	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[88], 1);commitHeight := version.NewHeight(txmgr.current.blockNum(), txmgr.current.maxTxNumber())
	txmgr.commitRWLock.Lock()
	logger.Debugf("Write lock acquired for committing updates to state database")
	if err := txmgr.db.ApplyPrivacyAwareUpdates(txmgr.current.batch, commitHeight); err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[95], 1);
		txmgr.commitRWLock.Unlock()
		return err
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[89], 1);txmgr.commitRWLock.Unlock()
	// only while holding a lock on oldBlockCommit, we should clear the cache as the
	// cache is being used by the old pvtData committer to load the version of
	// hashedKeys. Also, note that the PrepareForExpiringKeys uses the cache.
	txmgr.clearCache()
	logger.Debugf("Updates committed to state database and the write lock is released")

	// purge manager should be called (in this call the purge mgr removes the expiry entries from schedules) after committing to statedb
	if err := txmgr.pvtdataPurgeMgr.BlockCommitDone(); err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[96], 1);
		return err
	}
	// In the case of error state listeners will not recieve this call - instead a peer panic is caused by the ledger upon receiveing
	// an error from this function
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[90], 1);txmgr.updateStateListeners()
	return nil
}

// Rollback implements method in interface `txmgmt.TxMgr`
func (txmgr *LockBasedTxMgr) Rollback() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[97], 1);
	txmgr.reset()
}

// clearCache empty the cache maintained by the statedb implementation
func (txmgr *LockBasedTxMgr) clearCache() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[98], 1);
	if txmgr.db.IsBulkOptimizable() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[99], 1);
		txmgr.db.ClearCachedVersions()
	}
}

// ShouldRecover implements method in interface kvledger.Recoverer
func (txmgr *LockBasedTxMgr) ShouldRecover(lastAvailableBlock uint64) (bool, uint64, error) {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[100], 1);
	savepoint, err := txmgr.GetLastSavepoint()
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[103], 1);
		return false, 0, err
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[101], 1);if savepoint == nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[104], 1);
		return true, 0, nil
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[102], 1);return savepoint.BlockNum != lastAvailableBlock, savepoint.BlockNum + 1, nil
}

// CommitLostBlock implements method in interface kvledger.Recoverer
func (txmgr *LockBasedTxMgr) CommitLostBlock(blockAndPvtdata *ledger.BlockAndPvtData) error {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[105], 1);
	block := blockAndPvtdata.Block
	logger.Debugf("Constructing updateSet for the block %d", block.Header.Number)
	if _, err := txmgr.ValidateAndPrepare(blockAndPvtdata, false); err != nil {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[108], 1);
		return err
	}

	// log every 1000th block at Info level so that statedb rebuild progress can be tracked in production envs.
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[106], 1);if block.Header.Number%1000 == 0 {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[109], 1);
		logger.Infof("Recommitting block [%d] to state database", block.Header.Number)
	} else{ _cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[110], 1);{
		logger.Debugf("Recommitting block [%d] to state database", block.Header.Number)
	}}

	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[107], 1);return txmgr.Commit()
}

func extractStateUpdates(batch *privacyenabledstate.UpdateBatch, namespaces []string) ledger.StateUpdates {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[111], 1);
	stateupdates := make(ledger.StateUpdates)
	for _, namespace := range namespaces {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[113], 1);
		updatesMap := batch.PubUpdates.GetUpdates(namespace)
		var kvwrites []*kvrwset.KVWrite
		for key, versionedValue := range updatesMap {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[114], 1);
			kvwrites = append(kvwrites, &kvrwset.KVWrite{Key: key, IsDelete: versionedValue.Value == nil, Value: versionedValue.Value})
			if len(kvwrites) > 0 {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[115], 1);
				stateupdates[namespace] = kvwrites
			}
		}
	}
	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[112], 1);return stateupdates
}

func (txmgr *LockBasedTxMgr) updateStateListeners() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[116], 1);
	for _, l := range txmgr.current.listeners {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[117], 1);
		l.StateCommitDone(txmgr.ledgerid)
	}
}

func (txmgr *LockBasedTxMgr) reset() {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[118], 1);
	txmgr.current = nil
}

