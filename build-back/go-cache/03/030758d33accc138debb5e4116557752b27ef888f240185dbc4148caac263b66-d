// struct to hold info required for PKCS#8
type pkcs8Info struct {
	Version             int
	PrivateKeyAlgorithm []asn1.ObjectIdentifier
	PrivateKey          []byte
}

type ecPrivateKey struct {
	Version       int
	PrivateKey    []byte
	NamedCurveOID asn1.ObjectIdentifier `asn1:"optional,explicit,tag:0"`
	PublicKey     asn1.BitString        `asn1:"optional,explicit,tag:1"`
}

var (
	oidNamedCurveP224 = asn1.ObjectIdentifier{1, 3, 132, 0, 33}
	oidNamedCurveP256 = asn1.ObjectIdentifier{1, 2, 840, 10045, 3, 1, 7}
	oidNamedCurveP384 = asn1.ObjectIdentifier{1, 3, 132, 0, 34}
	oidNamedCurveP521 = asn1.ObjectIdentifier{1, 3, 132, 0, 35}
)

var oidPublicKeyECDSA = asn1.ObjectIdentifier{1, 2, 840, 10045, 2, 1}

func oidFromNamedCurve(curve elliptic.Curve) (asn1.ObjectIdentifier, bool) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[0], 1);
	switch curve {
	case elliptic.P224():_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[2], 1);
		return oidNamedCurveP224, true
	case elliptic.P256():_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[3], 1);
		return oidNamedCurveP256, true
	case elliptic.P384():_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[4], 1);
		return oidNamedCurveP384, true
	case elliptic.P521():_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[5], 1);
		return oidNamedCurveP521, true
	}
	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[1], 1);return nil, false
}

// PrivateKeyToDER marshals a private key to der
func PrivateKeyToDER(privateKey *ecdsa.PrivateKey) ([]byte, error) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[6], 1);
	if privateKey == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[8], 1);
		return nil, errors.New("Invalid ecdsa private key. It must be different from nil.")
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[7], 1);return x509.MarshalECPrivateKey(privateKey)
}

// PrivateKeyToPEM converts the private key to PEM format.
// EC private keys are converted to PKCS#8 format.
// RSA private keys are converted to PKCS#1 format.
func PrivateKeyToPEM(privateKey interface{}, pwd []byte) ([]byte, error) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[9], 1);
	// Validate inputs
	if len(pwd) != 0 {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[12], 1);
		return PrivateKeyToEncryptedPEM(privateKey, pwd)
	}
	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[10], 1);if privateKey == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[13], 1);
		return nil, errors.New("Invalid key. It must be different from nil.")
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[11], 1);switch k := privateKey.(type) {
	case *ecdsa.PrivateKey:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[14], 1);
		if k == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[22], 1);
			return nil, errors.New("Invalid ecdsa private key. It must be different from nil.")
		}

		// get the oid for the curve
		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[15], 1);oidNamedCurve, ok := oidFromNamedCurve(k.Curve)
		if !ok {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[23], 1);
			return nil, errors.New("unknown elliptic curve")
		}

		// based on https://golang.org/src/crypto/x509/sec1.go
		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[16], 1);privateKeyBytes := k.D.Bytes()
		paddedPrivateKey := make([]byte, (k.Curve.Params().N.BitLen()+7)/8)
		copy(paddedPrivateKey[len(paddedPrivateKey)-len(privateKeyBytes):], privateKeyBytes)
		// omit NamedCurveOID for compatibility as it's optional
		asn1Bytes, err := asn1.Marshal(ecPrivateKey{
			Version:    1,
			PrivateKey: paddedPrivateKey,
			PublicKey:  asn1.BitString{Bytes: elliptic.Marshal(k.Curve, k.X, k.Y)},
		})

		if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[24], 1);
			return nil, fmt.Errorf("error marshaling EC key to asn1 [%s]", err)
		}

		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[17], 1);var pkcs8Key pkcs8Info
		pkcs8Key.Version = 0
		pkcs8Key.PrivateKeyAlgorithm = make([]asn1.ObjectIdentifier, 2)
		pkcs8Key.PrivateKeyAlgorithm[0] = oidPublicKeyECDSA
		pkcs8Key.PrivateKeyAlgorithm[1] = oidNamedCurve
		pkcs8Key.PrivateKey = asn1Bytes

		pkcs8Bytes, err := asn1.Marshal(pkcs8Key)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[25], 1);
			return nil, fmt.Errorf("error marshaling EC key to asn1 [%s]", err)
		}
		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[18], 1);return pem.EncodeToMemory(
			&pem.Block{
				Type:  "PRIVATE KEY",
				Bytes: pkcs8Bytes,
			},
		), nil
	case *rsa.PrivateKey:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[19], 1);
		if k == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[26], 1);
			return nil, errors.New("Invalid rsa private key. It must be different from nil.")
		}
		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[20], 1);raw := x509.MarshalPKCS1PrivateKey(k)

		return pem.EncodeToMemory(
			&pem.Block{
				Type:  "RSA PRIVATE KEY",
				Bytes: raw,
			},
		), nil
	default:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[21], 1);
		return nil, errors.New("Invalid key type. It must be *ecdsa.PrivateKey or *rsa.PrivateKey")
	}
}

// PrivateKeyToEncryptedPEM converts a private key to an encrypted PEM
func PrivateKeyToEncryptedPEM(privateKey interface{}, pwd []byte) ([]byte, error) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[27], 1);
	if privateKey == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[29], 1);
		return nil, errors.New("Invalid private key. It must be different from nil.")
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[28], 1);switch k := privateKey.(type) {
	case *ecdsa.PrivateKey:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[30], 1);
		if k == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[35], 1);
			return nil, errors.New("Invalid ecdsa private key. It must be different from nil.")
		}
		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[31], 1);raw, err := x509.MarshalECPrivateKey(k)

		if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[36], 1);
			return nil, err
		}

		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[32], 1);block, err := x509.EncryptPEMBlock(
			rand.Reader,
			"PRIVATE KEY",
			raw,
			pwd,
			x509.PEMCipherAES256)

		if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[37], 1);
			return nil, err
		}

		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[33], 1);return pem.EncodeToMemory(block), nil

	default:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[34], 1);
		return nil, errors.New("Invalid key type. It must be *ecdsa.PrivateKey")
	}
}

// DERToPrivateKey unmarshals a der to private key
func DERToPrivateKey(der []byte) (key interface{}, err error) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[38], 1);

	if key, err = x509.ParsePKCS1PrivateKey(der); err == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[42], 1);
		return key, nil
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[39], 1);if key, err = x509.ParsePKCS8PrivateKey(der); err == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[43], 1);
		switch key.(type) {
		case *rsa.PrivateKey, *ecdsa.PrivateKey:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[44], 1);
			return
		default:_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[45], 1);
			return nil, errors.New("Found unknown private key type in PKCS#8 wrapping")
		}
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[40], 1);if key, err = x509.ParseECPrivateKey(der); err == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[46], 1);
		return
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[41], 1);return nil, errors.New("Invalid key type. The DER must contain an rsa.PrivateKey or ecdsa.PrivateKey")
}

// PEMtoPrivateKey unmarshals a pem to private key
func PEMtoPrivateKey(raw []byte, pwd []byte) (interface{}, error) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[47], 1);
	if len(raw) == 0 {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[52], 1);
		return nil, errors.New("Invalid PEM. It must be different from nil.")
	}
	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[48], 1);block, _ := pem.Decode(raw)
	if block == nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[53], 1);
		return nil, fmt.Errorf("Failed decoding PEM. Block must be different from nil. [% x]", raw)
	}

	// TODO: derive from header the type of the key

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[49], 1);if x509.IsEncryptedPEMBlock(block) {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[54], 1);
		if len(pwd) == 0 {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[58], 1);
			return nil, errors.New("Encrypted Key. Need a password")
		}

		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[55], 1);decrypted, err := x509.DecryptPEMBlock(block, pwd)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[59], 1);
			return nil, fmt.Errorf("Failed PEM decryption [%s]", err)
		}

		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[56], 1);key, err := DERToPrivateKey(decrypted)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[60], 1);
			return nil, err
		}
		_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[57], 1);return key, err
	}

	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[50], 1);cert, err := DERToPrivateKey(block.Bytes)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[61], 1);
		return nil, err
	}
	_cover_atomic_.AddUint32(&GoCover_3_656234373336356538323066.Count[51], 1);return cert, err
}

