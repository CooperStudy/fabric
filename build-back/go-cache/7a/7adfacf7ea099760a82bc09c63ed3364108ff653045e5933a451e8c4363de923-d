		return fmt.Errorf("Invalid policy name during check policy on signed data on channel [%s]. Name must be different from nil.", channelID)
	}

	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[35], 1);if sd == nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[41], 1);
		return fmt.Errorf("Invalid signed data during check policy on channel [%s] with policy [%s]", channelID, policyName)
	}

	// Get Policy
	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[36], 1);policyManager, _ := p.channelPolicyManagerGetter.Manager(channelID)
	if policyManager == nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[42], 1);
