// Attributes contains attribute names and values
type Attributes struct {
	Attrs map[string]string `json:"attrs"`
}

// Names returns the names of the attributes
func (a *Attributes) Names() []string {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[38], 1);
	i := 0
	names := make([]string, len(a.Attrs))
	for name := range a.Attrs {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[40], 1);
		names[i] = name
		i++
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[39], 1);return names
}

// Contains returns true if the named attribute is found
func (a *Attributes) Contains(name string) bool {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[41], 1);
	_, ok := a.Attrs[name]
	return ok
}

// Value returns an attribute's value
func (a *Attributes) Value(name string) (string, bool, error) {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[42], 1);
	attr, ok := a.Attrs[name]
	return attr, ok, nil
}

// True returns nil if the value of attribute 'name' is true;
// otherwise, an appropriate error is returned.
