	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[31], 1);
		return fmt.Errorf("Failed getting local MSP principal during channelless check policy with policy [%s]: [%s]", policyName, err)
	}

	// Verify that proposal's creator satisfies the principal
	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[23], 1);err = id.SatisfiesPrincipal(principal)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[32], 1);
		return fmt.Errorf("Failed verifying that proposal's creator satisfies local MSP principal during channelless check policy with policy [%s]: [%s]", policyName, err)
	}

	// Verify the signature
	_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[24], 1);return id.Verify(signedProp.ProposalBytes, signedProp.Signature)
}

// CheckPolicyBySignedData checks that the passed signed data is valid with the respect to
// passed policy on the passed channel.
func (p *policyChecker) CheckPolicyBySignedData(channelID, policyName string, sd []*common.SignedData) error {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[33], 1);
	if channelID == "" {_cover_atomic_.AddUint32(&GoCover_0_636166646564646431353166.Count[39], 1);
