// Get the attribute info from a certificate extension, or return nil if not found
func getAttributesFromCert(cert *x509.Certificate) ([]byte, error) {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[50], 1);
	for _, ext := range cert.Extensions {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[52], 1);
		if isAttrOID(ext.Id) {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[53], 1);
			return ext.Value, nil
		}
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[51], 1);return nil, nil
}

// Is the object ID equal to the attribute info object ID?
func isAttrOID(oid asn1.ObjectIdentifier) bool {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[54], 1);
	if len(oid) != len(AttrOID) {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[57], 1);
		return false
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[55], 1);for idx, val := range oid {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[58], 1);
		if val != AttrOID[idx] {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[59], 1);
			return false
		}
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[56], 1);return true
}

// Get an attribute from 'attrs' by its name, or nil if not found
func getAttrByName(name string, attrs []Attribute) Attribute {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[60], 1);
	for _, attr := range attrs {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[62], 1);
		if attr.GetName() == name {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[63], 1);
			return attr
		}
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[61], 1);return nil
}

