import (
	pb "github.com/hyperledger/fabric/protos/peer"
)

const (
	// OK constant - status code less than 400, endorser will endorse it.
	// OK means init or invoke successfully.
	OK = 200

	// ERRORTHRESHOLD constant - status code greater than or equal to 400 will be considered an error and rejected by endorser.
	ERRORTHRESHOLD = 400

	// ERROR constant - default error value
	ERROR = 500
)

func Success(payload []byte) pb.Response {_cover_atomic_.AddUint32(&GoCover_5_323061643534336238336134.Count[0], 1);
	return pb.Response{
		Status:  OK,
		Payload: payload,
	}
}

func Error(msg string) pb.Response {_cover_atomic_.AddUint32(&GoCover_5_323061643534336238336134.Count[1], 1);
	return pb.Response{
		Status:  ERROR,
		Message: msg,
	}
}

