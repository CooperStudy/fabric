	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[42], 1);b, err := proto.Marshal(block)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[48], 1);
		return err
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[43], 1);file := channelID + ".block"
	if outputBlock != common.UndefinedParamValue {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[49], 1);
		file = outputBlock
	}
	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[44], 1);err = ioutil.WriteFile(file, b, 0644)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[50], 1);
		return err
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[45], 1);return nil
}

func getGenesisBlock(cf *ChannelCmdFactory) (*cb.Block, error) {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[51], 1);
	timer := time.NewTimer(timeout)
	defer timer.Stop()

	for {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[52], 1);
		select {
		case <-timer.C:_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[53], 1);
			cf.DeliverClient.Close()
			return nil, errors.New("timeout waiting for channel creation")
		default:_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[54], 1);
			if block, err := cf.DeliverClient.GetSpecifiedBlock(0); err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[55], 1);
				cf.DeliverClient.Close()
				cf, err = InitCmdFactory(EndorserNotRequired, PeerDeliverNotRequired, OrdererRequired)
				if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[57], 1);
					return nil, errors.WithMessage(err, "failed connecting")
				}
				_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[56], 1);time.Sleep(200 * time.Millisecond)
			} else{ _cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[58], 1);{
				cf.DeliverClient.Close()
				return block, nil
			}}
		}
	}
}

