	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[44], 1);if !ok {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[48], 1);
		return fmt.Errorf("Attribute '%s' was not found", name)
	}
	_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[45], 1);if val != "true" {_cover_atomic_.AddUint32(&GoCover_0_613937353030653161323638.Count[49], 1);
