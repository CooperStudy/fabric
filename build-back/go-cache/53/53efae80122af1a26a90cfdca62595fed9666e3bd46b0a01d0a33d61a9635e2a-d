	var err error
	if result, err = itr.Next(); err != nil {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[6], 1);
		return false, err
	}
	_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[2], 1);if len(rqResults) == 0 {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[7], 1);
		return result == nil, nil
	}
	_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[3], 1);for i := 0; i < len(rqResults); i++ {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[8], 1);
		kvRead := rqResults[i]
		logger.Debugf("comparing kvRead=[%#v] to queryResponse=[%#v]", kvRead, result)
		if result == nil {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[12], 1);
			logger.Debugf("Query response nil. Key [%s] got deleted", kvRead.Key)
			return false, nil
		}
		_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[9], 1);versionedKV := result.(*statedb.VersionedKV)
		if versionedKV.Key != kvRead.Key {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[13], 1);
			logger.Debugf("key name mismatch: Key in rwset = [%s], key in query results = [%s]", kvRead.Key, versionedKV.Key)
			return false, nil
		}
		_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[10], 1);if !version.AreSame(versionedKV.Version, convertToVersionHeight(kvRead.Version)) {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[14], 1);
			logger.Debugf(`Version mismatch for key [%s]: Version in rwset = [%#v], latest version = [%#v]`,
				versionedKV.Key, versionedKV.Version, kvRead.Version)
			return false, nil
		}
		_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[11], 1);if result, err = itr.Next(); err != nil {_cover_atomic_.AddUint32(&GoCover_1_616464313537613636643832.Count[15], 1);
