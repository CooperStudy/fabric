	_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[14], 1);var proposalResp *pb.ProposalResponse
	proposalResp, err = cf.EndorserClient.ProcessProposal(context.Background(), signedProp)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[22], 1);
		return ProposalFailedErr(err.Error())
	}

	_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[15], 1);if proposalResp == nil {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[23], 1);
		return ProposalFailedErr("nil proposal response")
	}

	_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[16], 1);if proposalResp.Response.Status != 0 && proposalResp.Response.Status != 200 {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[24], 1);
		return ProposalFailedErr(fmt.Sprintf("bad proposal response %d: %s", proposalResp.Response.Status, proposalResp.Response.Message))
	}
