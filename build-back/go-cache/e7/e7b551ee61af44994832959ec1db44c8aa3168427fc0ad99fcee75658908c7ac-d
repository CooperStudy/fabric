	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[20], 1);
		return fmt.Errorf("Error creating proposal for join %s", err)
	}

	_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[13], 1);var signedProp *pb.SignedProposal
	signedProp, err = putils.GetSignedProposal(prop, cf.Signer)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[21], 1);
		return fmt.Errorf("Error creating signed proposal %s", err)
