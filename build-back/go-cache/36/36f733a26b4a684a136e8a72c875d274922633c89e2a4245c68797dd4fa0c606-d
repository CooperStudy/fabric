// ValidateTransaction checks that the transaction envelope is properly formed
func ValidateTransaction(e *common.Envelope, c channelconfig.ApplicationCapabilities) (*common.Payload, pb.TxValidationCode) {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[92], 1);
	putilsLogger.Debugf("ValidateTransactionEnvelope starts for envelope %p", e)

	// check for nil argument
	if e == nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[97], 1);
		putilsLogger.Errorf("Error: nil envelope")
		return nil, pb.TxValidationCode_NIL_ENVELOPE
	}

	// get the payload from the envelope
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[93], 1);payload, err := utils.GetPayload(e)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[98], 1);
		putilsLogger.Errorf("GetPayload returns err %s", err)
		return nil, pb.TxValidationCode_BAD_PAYLOAD
	}

	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[94], 1);putilsLogger.Debugf("Header is %s", payload.Header)

	// validate the header
	chdr, shdr, err := validateCommonHeader(payload.Header)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[99], 1);
		putilsLogger.Errorf("validateCommonHeader returns err %s", err)
		return nil, pb.TxValidationCode_BAD_COMMON_HEADER
	}

	// validate the signature in the envelope
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[95], 1);err = checkSignatureFromCreator(shdr.Creator, e.Signature, e.Payload, chdr.ChannelId)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[100], 1);
		putilsLogger.Errorf("checkSignatureFromCreator returns err %s", err)
		return nil, pb.TxValidationCode_BAD_CREATOR_SIGNATURE
	}

	// TODO: ensure that creator can transact with us (some ACLs?) which set of APIs is supposed to give us this info?

	// continue the validation in a way that depends on the type specified in the header
	_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[96], 1);switch common.HeaderType(chdr.Type) {
	case common.HeaderType_ENDORSER_TRANSACTION:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[101], 1);
		// Verify that the transaction ID has been computed properly.
		// This check is needed to ensure that the lookup into the ledger
		// for the same TxID catches duplicates.
		err = utils.CheckTxID(
			chdr.TxId,
			shdr.Nonce,
			shdr.Creator)

		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[107], 1);
			putilsLogger.Errorf("CheckTxID returns err %s", err)
			return nil, pb.TxValidationCode_BAD_PROPOSAL_TXID
		}

		_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[102], 1);err = validateEndorserTransaction(payload.Data, payload.Header)
		putilsLogger.Debugf("ValidateTransactionEnvelope returns err %s", err)

		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[108], 1);
			putilsLogger.Errorf("validateEndorserTransaction returns err %s", err)
			return payload, pb.TxValidationCode_INVALID_ENDORSER_TRANSACTION
		} else{ _cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[109], 1);{
			return payload, pb.TxValidationCode_VALID
		}}
	case common.HeaderType_CONFIG:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[103], 1);
		// Config transactions have signatures inside which will be validated, especially at genesis there may be no creator or
		// signature on the outermost envelope

		err = validateConfigTransaction(payload.Data, payload.Header)

		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[110], 1);
			putilsLogger.Errorf("validateConfigTransaction returns err %s", err)
			return payload, pb.TxValidationCode_INVALID_CONFIG_TRANSACTION
		} else{ _cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[111], 1);{
			return payload, pb.TxValidationCode_VALID
		}}
	case common.HeaderType_TOKEN_TRANSACTION:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[104], 1);
		// Verify that the transaction ID has been computed properly.
		// This check is needed to ensure that the lookup into the ledger
		// for the same TxID catches duplicates.
		err = utils.CheckTxID(
			chdr.TxId,
			shdr.Nonce,
			shdr.Creator)

		if err != nil {_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[112], 1);
			putilsLogger.Errorf("CheckTxID returns err %s", err)
			return nil, pb.TxValidationCode_BAD_PROPOSAL_TXID
		}

		_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[105], 1);return payload, pb.TxValidationCode_VALID
	default:_cover_atomic_.AddUint32(&GoCover_0_393339656439646532633138.Count[106], 1);
		return nil, pb.TxValidationCode_UNSUPPORTED_TX_PAYLOAD
	}
}

