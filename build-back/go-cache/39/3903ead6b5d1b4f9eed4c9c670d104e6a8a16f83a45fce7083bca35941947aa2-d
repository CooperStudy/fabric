		return errors.New("must supply channel ID")
	}

	// Parsing of the command line is done so silence cmd usage
	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[60], 1);cmd.SilenceUsage = true

	var err error
	if cf == nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[63], 1);
		cf, err = InitCmdFactory(EndorserNotRequired, PeerDeliverNotRequired, OrdererRequired)
		if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[64], 1);
			return err
		}
	}
	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[61], 1);return executeCreate(cf)
}

