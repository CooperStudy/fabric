// Get the DN (distinguished name) associated with a pkix.Name.
// NOTE: This code is almost a direct copy of the String() function in
// https://go-review.googlesource.com/c/go/+/67270/1/src/crypto/x509/pkix/pkix.go#26
// which returns a DN as defined by RFC 2253.
func getDN(name *pkix.Name) string {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[50], 1);
	r := name.ToRDNSequence()
	s := ""
	for i := 0; i < len(r); i++ {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[52], 1);
		rdn := r[len(r)-1-i]
		if i > 0 {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[54], 1);
			s += ","
		}
		_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[53], 1);for j, tv := range rdn {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[55], 1);
			if j > 0 {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[59], 1);
				s += "+"
			}
			_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[56], 1);typeString := tv.Type.String()
			typeName, ok := attributeTypeNames[typeString]
			if !ok {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[60], 1);
				derBytes, err := asn1.Marshal(tv.Value)
				if err == nil {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[62], 1);
					s += typeString + "=#" + hex.EncodeToString(derBytes)
					continue // No value escaping necessary.
				}
				_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[61], 1);typeName = typeString
			}
			_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[57], 1);valueString := fmt.Sprint(tv.Value)
			escaped := ""
			begin := 0
			for idx, c := range valueString {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[63], 1);
				if (idx == 0 && (c == ' ' || c == '#')) ||
					(idx == len(valueString)-1 && c == ' ') {_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[65], 1);
					escaped += valueString[begin:idx]
					escaped += "\\" + string(c)
					begin = idx + 1
					continue
				}
				_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[64], 1);switch c {
				case ',', '+', '"', '\\', '<', '>', ';':_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[66], 1);
					escaped += valueString[begin:idx]
					escaped += "\\" + string(c)
					begin = idx + 1
				}
			}
			_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[58], 1);escaped += valueString[begin:]
			s += typeName + "=" + escaped
		}
	}
	_cover_atomic_.AddUint32(&GoCover_0_653664636166313238303538.Count[51], 1);return s
}

var attributeTypeNames = map[string]string{
	"2.5.4.6":  "C",
	"2.5.4.10": "O",
	"2.5.4.11": "OU",
	"2.5.4.3":  "CN",
	"2.5.4.5":  "SERIALNUMBER",
	"2.5.4.7":  "L",
	"2.5.4.8":  "ST",
	"2.5.4.9":  "STREET",
	"2.5.4.17": "POSTALCODE",
}

