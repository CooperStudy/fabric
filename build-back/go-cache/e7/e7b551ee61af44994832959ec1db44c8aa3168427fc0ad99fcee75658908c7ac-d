func getJoinCCSpec() (*pb.ChaincodeSpec, error) {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[5], 1);
	if genesisBlockPath == common.UndefinedParamValue {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[8], 1);
		return nil, errors.New("Must supply genesis block file")
	}

	_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[6], 1);gb, err := ioutil.ReadFile(genesisBlockPath)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[9], 1);
		return nil, GBFileNotFoundErr(err.Error())
	}
	// Build the spec
	_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[7], 1);input := &pb.ChaincodeInput{Args: [][]byte{[]byte(cscc.JoinChain), gb}}

	spec := &pb.ChaincodeSpec{
		Type:        pb.ChaincodeSpec_Type(pb.ChaincodeSpec_Type_value["GOLANG"]),
		ChaincodeId: &pb.ChaincodeID{Name: "cscc"},
		Input:       input,
	}

	return spec, nil
}

func executeJoin(cf *ChannelCmdFactory) (err error) {_cover_atomic_.AddUint32(&GoCover_4_613931643833636437303432.Count[10], 1);
