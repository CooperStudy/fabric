		Namespace:      ns,
		CollectionName: collPvtData.CollectionName,
	}

	for _, kvWrite := range kvRWSet.Writes {_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[44], 1); // for each kv pair
		hashedCompositeKey.KeyHash = string(util.ComputeStringHash(kvWrite.Key))
		uniquePvtData.updateUsingPvtWrite(kvWrite, hashedCompositeKey, ver)
	} // for each kv pair

	_cover_atomic_.AddUint32(&GoCover_4_386331373365306636336339.Count[42], 1);return nil
