		return err
	}

	_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[41], 1);block, err := getGenesisBlock(cf)
	if err != nil {_cover_atomic_.AddUint32(&GoCover_1_613931643833636437303432.Count[47], 1);
		return err
